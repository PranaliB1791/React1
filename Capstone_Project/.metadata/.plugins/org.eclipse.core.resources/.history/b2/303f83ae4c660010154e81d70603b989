package com.example.demo.dto;

import com.fasterxml.jackson.annotation.JsonProperty;

public class PlanDto {
	private Long id;

	@JsonProperty("plan_name") // <-- This maps plan_name from JSON to this field
	private String name;

	private Double price;

	public PlanDto() {
	}

	public PlanDto(Long id, String name, Double price) {
		this.id = id;
		this.name = name;
		this.price = price;
	}

	// Getters and Setters
	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Double getPrice() {
		return price;
	}

	public void setPrice(Double price) {
		this.price = price;
	}
}
